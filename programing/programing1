package boram;

  public class Student {   
  
   String major;    
   int no;   
   
  public static void main(String[] args) {        
    Student student = new Student();       
    student.major = "컴퓨터정보공학과";        
    student.no = 20161850;    
    
    System.out.println("학번: " + student.no + ", 학과: " + student.major);   
  }
}
